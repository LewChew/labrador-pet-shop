{
  "name": "labrador-pet-shop",
  "version": "0.1.0",
  "description": "A pet shop project specialized in Labrador retrievers and their needs",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "server": "nodemon server/index.js",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "test": "jest"
  },
  "keywords": [
    "labrador",
    "pet-shop",
    "dogs",
    "e-commerce"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "stripe": "^13.3.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
